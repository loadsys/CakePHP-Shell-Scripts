{
    "name": "loadsys/cakephp-shell-scripts",
    "description": "Reusable CakePHP project shell scripts.",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Porter",
            "email": "beporter@users.sourceforge.net"
        }
    ],
    "require": {
        "phpunit/phpunit": "3.7.*",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "loadsys/loadsys_codesniffer": "~1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "clear-cache",
        "clear-logs",
        "composer-install",
        "coverage-ensure",
        "db-backup",
        "db-credentials",
        "db-loadschema",
        "db-login",
        "db-sample-data",
        "db-seed",
        "db-showupdates",
        "deps-install",
        "deps-update",
        "docs-generate",
        "email",
        "folder-delete-items-older-than-days",
        "git-currentbranch",
        "git-hook-workingcopychange",
        "git-localchanges",
        "git-remotechanges",
        "git-submodules",
        "init-repo",
        "migrations",
        "nfsexports-setsudoaccess",
        "pear-install-deps",
        "pear-package-installed",
        "pear-package-installed-dumb",
        "rename-empty",
        "run-codesniffer",
        "run-in-vagrant",
        "run-tests",
        "semver-get-pointrelease",
        "set-owner",
        "tmp-dir-permissions",
        "update",
        "updatewritedirs",
        "writedirs"
    ]
}