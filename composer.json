{
    "name": "loadsys/cakephp-shell-scripts",
    "description": "This collection of scripts is intended to provide consistency and shortcuts for common project tasks (currently they are tuned for CakePHP v2.x projects, pre-Composer).",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Porter",
            "email": "beporter@users.sourceforge.net"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "add-cakephp-version",
        "bin-selfupdate",
        "clear-cache",
        "clear-logs",
        "db-backup",
        "db-credentials",
        "db-loadschema",
        "db-login",
        "db-sample-data",
        "db-showupdates",
        "db-writeconfig",
        "docs-generate",
        "email",
        "find-symlinks-to-in",
        "git-currentbranch",
        "git-localchanges",
        "git-remotechanges",
        "git-submodules",
        "init-repo",
        "migrations",
        "paths",
        "rename-empty",
        "run-codesniffer",
        "run-tests",
        "set-configs",
        "set-owner",
        "symlink-cake-core",
        "template",
        "update",
        "updatewritedirs",
        "writedirs"
    ]
}