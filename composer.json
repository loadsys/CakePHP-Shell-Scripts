{
    "name": "loadsys/cakephp-shell-scripts",
    "description": "Reusable CakePHP project shell scripts.",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Porter",
            "email": "beporter@users.sourceforge.net"
        }
    ],
    "require": {
        "phpunit/phpunit": "~4.1",
        "phpdocumentor/phpdocumentor": "~2.0",
        "squizlabs/php_codesniffer": "~2.0",
        "loadsys/cakephp-config-read": "~3.0",
        "loadsys/loadsys_codesniffer": "~3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "cache-clear",
        "codesniffer-run",
        "composer-install",
        "coverage-ensure",
        "db-backup",
        "db-credentials",
        "db-import",
        "db-login",
        "db-sample-data",
        "deploy",
        "docs-generate",
        "email",
        "folder-delete-items-older-than-days",
        "git-currentbranch",
        "git-hook-workingcopychange",
        "git-localchanges",
        "git-remotechanges",
        "logs-clear",
        "migrations",
        "nfsexports-setsudoaccess",
        "owner-set",
        "rename-empty",
        "tests-run",
        "vagrant-exec"
    ]
}
