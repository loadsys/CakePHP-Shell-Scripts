#!/usr/bin/env bash
# Helper script. Not meant for direct use. Loads shared variables into another bash script.
#
# Usage in other scripts: `source ./env-vars-load [env]`
#
# If the [env] argument is not provided, the value from APP_ENV will be used.

VARS=( "$(./env-vars-echo bash)" )
for i in "${VARS[@]}"; do
	eval $i
done
